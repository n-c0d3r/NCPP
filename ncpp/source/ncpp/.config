
#cmakedefine NCPP_VERSION @NCPP_VERSION@
#define NCPP_VERSION_STR "@NCPP_VERSION@"
#cmakedefine NCPP_VERSION_WITH_UNDERSCORE @NCPP_VERSION_WITH_UNDERSCORE@
#define NCPP_VERSION_WITH_UNDERSCORE_STR "@NCPP_VERSION_WITH_UNDERSCORE@"
#define NCPP_VERSION_@NCPP_VERSION_WITH_UNDERSCORE@
#cmakedefine NCPP_VERSION_NUMBER @NCPP_VERSION_NUMBER@
#cmakedefine NCPP_MAX_VERSION_COMPONENT_VALUE @NCPP_MAX_VERSION_COMPONENT_VALUE@
#define NCPP_COMPUTE_VERSION_VALUE(major, minor, patch) (major * (NCPP_MAX_VERSION_COMPONENT_VALUE + 1) * (NCPP_MAX_VERSION_COMPONENT_VALUE + 1) + minor * (NCPP_MAX_VERSION_COMPONENT_VALUE + 1) + patch)

// For mem
#cmakedefine NCPP_ENABLE_MEMORY_COUNTING @NCPP_ENABLE_MEMORY_COUNTING@
#cmakedefine NCPP_DEFAULT_ALLOCATOR @NCPP_DEFAULT_ALLOCATOR@

// For pac
#cmakedefine NCPP_ENABLE_WEAKLOCK_WARNING @NCPP_ENABLE_WEAKLOCK_WARNING@

// For logging
#cmakedefine NCPP_TAB_SIZE @NCPP_TAB_SIZE@
#cmakedefine NCPP_MAX_TAB_COUNT @NCPP_MAX_TAB_COUNT@

// For profiler
#cmakedefine NCPP_ENABLE_SCOPED_PROFILE @NCPP_ENABLE_SCOPED_PROFILE@

// For rtti
#cmakedefine NCPP_ENABLE_RTTI @NCPP_ENABLE_RTTI@
#cmakedefine NCPP_ENABLE_METADATA @NCPP_ENABLE_METADATA@